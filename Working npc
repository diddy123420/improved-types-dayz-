	</attachments>	
	<attachments>
		<item name="M4A1" />
	</attachments>
	<attachments chance="1.00">
		<item name="M4_OEBttstck" chance="1.00" />
	</attachments>
	<attachments chance="1.00">
		<item name="M4_RISHndgrd" chance="1.00" />
	</attachments>
	<attachments chance="1.00">
		<item name="ACOGOptic" chance="1.00" />
